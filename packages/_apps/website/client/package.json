{
    "name": "@dieta/website-client",
    "version": "1.0.0",
    "description": "",
    "main": "dist/app/index.js",
    "module": "dist/app/index.js",
    "typings": "dist/@types/src/index.d.ts",
    "directories": {
        "dist/@types": "dist/@types",
        "dist/lib": "dist/app"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" ",
        "build:all": "concurrently \"npm:build:typings\" \"npm:build:app\"",
        "build:all:dev": "concurrently \"npm:build:typings:dev\" \"npm:build:app:dev\"",
        "start": "next start ./sources/src -p 3000",
        "build:app": "next build ./sources/src -p 3000",
        "build:app:dev": "next dev ./sources/src",
        "build:typings": "babel ./sources/@types --out-dir ./dist/@types --copy-files",
        "build:typings:dev": "npm run build:typings -- --watch",
        "clean": "concurrently \"npm:clean:tsbuild\" \"npm:clean:sources\"",
        "clean:sources": "rimraf dist/{app,@types}/**",
        "clean:tsbuild": "rimraf dist/tsconfig.tsbuildinfo"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/preset-react": "^7.16.7",
        "@reduxjs/toolkit": "^1.6.2",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@dieta/common": "1.0.0",
        "axios": "^0.19.0",
        "next": "^12.1.4",
        "react": "18.0.0",
        "react-dom": "^18.0.0",
        "react-redux": "^7.2.5",
        "redux": "^4.1.2",
        "redux-dynamic-modules": "^5.2.3",
        "redux-dynamic-modules-saga": "^5.2.3"
    }
}
